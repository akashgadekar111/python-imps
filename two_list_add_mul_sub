a = [1,2,3,4]
b = [3,4,5,6,7]
c = []
d = []
e = []

#multiplication of list items
for i in range(0,len(a)):
    c.append(a[i]*b[i])
print(c)


#addition of list items
for i in range(0,len(a)):
    d.append(a[i]+b[i])
print(d)


#substraction of list items
for i in range(0,len(a)):
    e.append(a[i]-b[i])
print(e)
